        <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>

<body>
    <form id="form1" runat="server">
    <ajax:UpdatePanel ID="updTopo" runat="server">
        <ContentTemplate>
                    	        <asp:ScriptManager ID="ScriptManager1" runat="server">
                        </asp:ScriptManager>
        <div id="conteudoTop"><!-- Início da Div Conteúdo -->
	
		<div id="cabecalho"><!-- Início da Div Cabecalho -->
		     
            <div id="barraLogin"><!-- Início da Div Barra Login, contem o Login e Senha, Barra e Símbolo do Colégio -->   
				
                <div id="login"><!-- Início da Div de Login e Senha -->
        			<p>
        			<asp:Label ID="lblLogin" runat="server" Text="Login:" />
        			<asp:TextBox runat="server" ID="txtLogin" EnableViewState="false"
                                class="input" MaxLength="20" /> 
        			<asp:Label ID="lblSenha" runat="server" Text="Senha:" />
        			<asp:TextBox runat="server" type="text" ID="txtSenha" EnableViewState="false"
                                class="input" TextMode="Password" MaxLength="20" />
                    <asp:Button ID="btnLogar" Text="Logar" runat="server" onclick="btnLogar_Click"/>
                    <asp:LinkButton ID="lnkDeslogar" runat="server" Text="Deslogar" 
                            onclick="lnkDeslogar_Click" />
        			</p>
        		</div><!-- Fim Login -->
                
            	<div id="simboloCabecalho"></div><!-- Símbolo do Colégio -->

			</div><!-- Fim BarraLogin -->
		</div><!-- Fim Cabecalho -->
        
		<div class="menu"><!-- Inicio da Div do Menu Principal -->
            	<ul>
                	<li><a href="#">Colégio</a>
                        <ul>
                            <li><asp:LinkButton runat="server" ID="lkbHistorico" PostBackUrl="~/colegioHistorico.aspx">Histórico</asp:LinkButton></li>  
                            <li><asp:LinkButton runat="server" ID="lkbPrincipios" PostBackUrl="~/colegioPrincipios.aspx">Princípios</asp:LinkButton></li> 
                            <li><asp:LinkButton runat="server" ID="lkbInfra" PostBackUrl="~/colegioInfraEstrutura.aspx">Infra-Estrutura</asp:LinkButton></li> 
                        </ul>
                    </li>                  
                    <li><asp:LinkButton runat="server" ID="lkbEdInfantil" PostBackUrl="~/colegioConstrucao.aspx">Educaç&atilde;o 
                        Infantil</asp:LinkButton></li>
                    <li><asp:LinkButton runat="server" ID="lkbFundamental1" PostBackUrl="~/colegioFundamental1.aspx">Fundamental 
                        I</asp:LinkButton></li>
                    <li><asp:LinkButton runat="server" ID="lkbFundamental2" PostBackUrl="~/colegioFundamental2.aspx">Fundamental 
                        II</asp:LinkButton></li>                  
                    <li><asp:LinkButton runat="server" ID="lkbMedio" PostBackUrl="~/colegioMedio.aspx">Ensino<br /> 
                        Médio</asp:LinkButton></li>
                    <li><asp:LinkButton runat="server" ID="lkbAtividades" PostBackUrl="~/colegioAtividades.aspx">Atividades</asp:LinkButton></li>
                    <li><asp:LinkButton runat="server" ID="lkbExAlunos" PostBackUrl="~/colegioConstrucao.aspx">Ex-Alunos</asp:LinkButton></li>
                    <li><asp:LinkButton runat="server" ID="LkbContato" PostBackUrl="~/colegioContato.aspx">Contato</asp:LinkButton></li>                                 
  		  </ul>
		</div><!-- Fim Menu Principal -->
		</div>
		        </ContentTemplate>
    </ajax:UpdatePanel>
    <ajax:UpdatePanel ID="updMaster" runat="server">
        <ContentTemplate>
                <!-- Inicio do Conteúdo Principal da Página -->
                    <asp:ContentPlaceHolder ID="corpo" runat="server">
            </asp:ContentPlaceHolder>
        </ContentTemplate>
    </ajax:UpdatePanel>
    
            <table width="100%">
        <tr>
            <td align="center">
                <ajax:UpdateProgress ID="UpdateProgress1" Visible="true" runat="server" DisplayAfter="200"
                    AssociatedUpdatePanelID="updMaster">
                    <ProgressTemplate>
                        Aguarde...
                        <asp:Image ID="Image1" runat="server" 
                            ImageUrl="~/App_Themes/Default/Imagens/loading.gif" />
                    </ProgressTemplate>
                </ajax:UpdateProgress>
                <ajax:UpdateProgress ID="UpdateProgress2" Visible="true" runat="server" DisplayAfter="200"
                    AssociatedUpdatePanelID="updTopo">
                    <ProgressTemplate>
                        Aguarde...
                        <asp:Image ID="Image2" runat="server" 
                            ImageUrl="~/App_Themes/Default/Imagens/loading.gif" />
                    </ProgressTemplate>
                </ajax:UpdateProgress>
            </td>
        </tr>
    </table>
   </form>
</body>
</html>




